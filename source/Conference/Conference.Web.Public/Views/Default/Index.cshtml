@model IEnumerable<Registration.ReadModel.ConferenceAliasDTO>
@{
    ViewBag.Title = "Index";
}
<h2>
    Index</h2>
<p>
    Go to the specific conference page:
</p>
<ul>
@foreach (var conference in Model) {
    <li>@Html.ActionLink(conference.Name, "Display", "Conference", new { conferenceCode = conference.Code }, null)</li>
}
</ul>
